#:kivy 1.9.1

<WirelessConfigView>:
    orientation: 'vertical'
    spacing: dp(5)
    ScrollContainer:
        id: scroller
        size_hint_y: 1
        do_scroll_x: False
        StackLayout:
            id: wireless_settings
            height: max(self.minimum_height, scroller.height)
            size_hint: [1, None]
            spacing: [0, dp(20)]
            padding: dp(20)
